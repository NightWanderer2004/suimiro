{% set posts = collections.posts %}

<h1 class="title">{{ pageTitle }}</h1>

<ul class="postsList">
   {% for post in posts %}
   <li>
      <a class="postLink" href="{{ post.url }}">
         <h3>{{ post.data.title }}</h3>
      </a>
   </li>
   {% endfor %}
</ul>
